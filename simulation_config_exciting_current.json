{
  "manifest": {
    "$BASE_DIR": "${configdir}",
    "$OUTPUT_DIR": "$BASE_DIR/output_currents_baseline_blocked_server",
    "$COMPONENTS_DIR": "$BASE_DIR/components"
  },
  "target_simulator": "NEURON",
  "run": {
    "tstart": 0.0,
    "tstop": 15000,
    "dt": 0.1,
    "dL": 200000.0,
    "spike_threshold": -15.0,
    "nsteps_block": 5000
  },
  "conditions": {
    "celsius": 31.0,
    "v_init": -70.0
  },
  "inputs": {
    "thalamus_pyr_A_spikes": {
      "input_type": "spikes",
      "module": "sonata",
      "input_file": "$BASE_DIR/inputs/thalamus_pyr_A_spikes.h5",
      "node_set": "thalamus_pyr_A"
    },
    "thalamus_pyr_C_spikes": {
      "input_type": "spikes",
      "module": "sonata",
      "input_file": "$BASE_DIR/inputs/thalamus_pyr_C_spikes.h5",
      "node_set": "thalamus_pyr_C"
    },
    "thalamus_pv_spikes": {
      "input_type": "spikes",
      "module": "sonata",
      "input_file": "$BASE_DIR/inputs/thalamus_pv_spikes.h5",
      "node_set": "thalamus_pv"
    },
    "thalamus_som_spikes": {
      "input_type": "spikes",
      "module": "sonata",
      "input_file": "$BASE_DIR/inputs/thalamus_som_spikes.h5",
      "node_set": "thalamus_som"
    },
    "tone_spikes": {
      "input_type": "spikes",
      "module": "csv",
      "input_file": "$BASE_DIR/inputs/tone_spikes_baseline.csv",
      "node_set": "tone"
    }
  },
